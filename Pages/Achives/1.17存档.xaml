	<local:MyCard Margin="0,0,0,15" Title="1.17.1" CanSwap="True" IsSwaped="True">
		<!--Header-->
		<StackPanel Margin="30,40,30,20">
			<Border Style="{StaticResource verImgBdr}">
				<Border.Background>
					<ImageBrush ImageSource="https://www.lightbeacon.top/pnh/newsimgs/1_17/1_17_1/1-17-1-release-header.jpg" Stretch="Uniform" />
				</Border.Background>
				<Image Source="https://www.lightbeacon.top/pnh/newsimgs/1_17/1_17_1/1-17-1-release-header.jpg" Opacity="0" />
			</Border>
			<Border Style="{StaticResource verTxtBdr}">
				<TextBlock Style="{StaticResource verTxtBlk}" Text="1.17.1" />
			</Border>
			<!--Detail-->
			<TextBlock Text="更改" Style="{StaticResource h2}" />

			<StackPanel Style="{StaticResource ListStyle}">

				<Ellipse />
				<TextBlock Style="{StaticResource ListItem}" Text="蓝色美西螈现在只能通过繁育获得。" />

				<Ellipse />
				<TextBlock Style="{StaticResource ListItem}" Text="非尖叫类型的山羊现在有一定几率繁育出尖叫山羊。" />

				<Ellipse />
				<TextBlock Style="{StaticResource ListItem}" Text="现在在山羊跳跃或冲撞时也能向其施加状态效果。" />

				<Ellipse />
				<TextBlock Style="{StaticResource ListItem}" Text="将溺尸掉落铜锭的概率提升为 11% + 2% * 抢夺等级。" />

				<Ellipse />
				<TextBlock Style="{StaticResource ListItem}" Text="细雪现在在炼药锅的积累速度是原来的两倍（不过还是很慢！）。" />

				<Ellipse />
				<TextBlock Style="{StaticResource ListItem}" Text="僵尸，僵尸村民，尸壳和溺尸现在不再会拾起荧光墨囊。" />

			</StackPanel>

			<TextBlock Text="技术性修改" Style="{StaticResource h2}" />

			<StackPanel Style="{StaticResource ListStyle}">

				<Ellipse />
				<TextBlock Style="{StaticResource ListItem}" Text="被命名过的生物死亡会记录到日志。" />

			</StackPanel>

			<TextBlock Text="该版本修复了42个Bug" Style="{StaticResource footnote}" />
			<!--Logs-->

			<GroupBox Header="阅读完整日志" BorderBrush="{DynamicResource ColorBrush3}">
				<Grid UseLayoutRounding="True" Margin="10,10,10,10">
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="20*"></ColumnDefinition>
						<ColumnDefinition Width="10*"></ColumnDefinition>
						<ColumnDefinition Width="10*"></ColumnDefinition>
					</Grid.ColumnDefinitions>
					<local:MyButton Style="{StaticResource Read0}" EventData="https://minecraft.fandom.com/zh/wiki/Java版1.17.1" ColorType="Highlight" />
					<local:MyButton Style="{StaticResource Read1}" EventData="https://www.mcbbs.net/thread-1219658-1-1.html" />
					<local:MyButton Style="{StaticResource Read2}" EventData="https://www.minecraft.net/zh-hans/article/minecraft-java-edition-1-17-1" />
				</Grid>
			</GroupBox>
		</StackPanel>
	</local:MyCard>


